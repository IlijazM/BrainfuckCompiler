package compile;

class _Equals extends Command {

	@Override
	public String name() {
		return "equ";
	}
	
	@Override
	public String description() {
		return "equ out_var var var";
	}

	@Override
	public void convert(String[] args) throws IndexOutOfBoundsException, NumberFormatException {
		int x = findTempMem();
		int y = findTempMem();
		
		set(x, getPointer(args[1]));
	}

}
